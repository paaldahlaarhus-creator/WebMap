library(sf)
library(leaflet)
# Les GeoJSON
kommuner <- st_read("www/assets/kommuner.geojson", quiet =TRUE)
# Sett CRS hvis mangler (GeoJSON fra kartverket er som oftest EPSG:4258)
if (is.na(st_crs(kommuner))) {
st_crs(kommuner) <- 4258
}
# TransformÃ©r til WGS84 (Leaflet krever EPSG:4326)
kommuner <- st_transform(kommuner, 4326)
# Lag kart
leaflet(kommuner) |>
addTiles() |>
addPolygons(
label = ~kommunenavn,
fillOpacity = 0.4,
color = "blue",
weight = 1
)
